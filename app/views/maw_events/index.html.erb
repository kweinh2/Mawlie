<h1>Listing maw_events</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Where</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maw_events.each do |maw_event| %>
      <tr>
        <td><%= maw_event.date %></td>
        <td><%= maw_event.where %></td>
        <td><%= maw_event.name %></td>
        <td><%= link_to 'Show', maw_event %></td>
        <td><%= link_to 'Edit', edit_maw_event_path(maw_event) %></td>
        <td><%= link_to 'Destroy', maw_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Maw event', new_maw_event_path %>
